{
  "name": "nomic-boardroom",
  "version": "1.0.0",
  "description": "Nomic boardroom contracts.",
  "main": "src/index.js",
  "scripts": {
    "build:contracts": "wafr ./contracts/ --output ./contracts/lib/contracts.json",
    "deploy:ropsten": "npm run build:contracts && babel-node ./contracts/lib/deploy.js"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=6.5.0"
  },
  "keywords": [
    "yoyo",
    "boilerplate"
  ],
  "author": "Nick Dodson",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "ethjs": "0.2.6",
    "ethjs-provider-signer": "0.1.4",
    "ethjs-signer": "0.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "lint-staged": "3.4.0",
    "mocha": "3.2.0",
    "mocha-babel": "3.0.3",
    "offline-plugin": "4.6.2",
    "plop": "1.7.4",
    "pre-commit": "1.2.2",
    "regenerator-runtime": "0.10.3",
    "wafr": "^0.3.5",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  }
}
